{
  "name": "lgbtq.css",
  "version": "2.0.3",
  "description": "Add some Pride to your stylesheets",
  "main": "dist/lgbtq.css",
  "browser": "dist/lgbtq.css",
  "scripts": {
    "styles": "yarn styles:banner && yarn styles:bg",
    "styles:banner": "postcss src/banner.css -o dist/lgbtq-banners.css",
    "styles:bg": "postcss src/bg.css -o dist/lgbtq.css",
    "prepublish": "yarn styles"
  },
  "author": "charles peters <charlespeters42@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "postcss-apply": "^0.12.0",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1"
  },
  "browserslist": [
    "last 3 versions",
    "> 5%",
    "ff >= 10"
  ],
  "postcss": {
    "map": false,
    "plugins": {
      "postcss-import": {},
      "postcss-apply": {},
      "postcss-preset-env": {},
      "postcss-reporter": {
        "clearMessages": true
      }
    }
  },
  "repository": "charliewilco/lgbtq.css",
  "keywords": [
    "pride",
    "css",
    "gradients"
  ],
  "bugs": {
    "url": "https://github.com/charlespeters/lgbtq.css/issues"
  },
  "homepage": "https://github.com/charlespeters/lgbtq.css#readme"
}
