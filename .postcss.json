{
  "use": [
    "postcss-cssnext",
    "postcss-apply",
    "postcss-reporter"
  ],
  "input": "./index.css",
  "output": "./lgbtq.css",
  "local-plugins": true,
  "postcss-cssnext": {
    "warnForDuplicates": false,
    "features": {
      "rem": false
    }
  },
  "postcss-reporter": {
    "clearMessages": true
  }
}
